{
  "name": "package-template",
  "version": "1.0.0",
  "description": "A Node Package Template for Friends.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c --environment NODE_ENV:production",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:dts": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "keywords": [],
  "author": "Raven Satir",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voorjaar/package-template.git"
  },
  "bugs": {
    "url": "https://github.com/voorjaar/package-template/issues"
  },
  "homepage": "https://github.com/voorjaar/package-template#readme",
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@jridgewell/sourcemap-codec": "^1.4.14",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@sucrase/jest-plugin": "^3.0.0",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "rollup": "^3.7.5",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.0.0",
    "sucrase": "^3.29.0",
    "typescript": "^4.9.4"
  }
}
